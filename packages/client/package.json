{
  "name": "@use-telegram-auth/client",
  "version": "1.0.0",
  "description": "Core client-side functionality of authenticating users with Telegram without the widget",
  "author": "baloo",
  "license": "ISC",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
